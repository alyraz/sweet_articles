<% @categories.each do |category| %>
  <h3><%= category.name %></h3>
  <ul>
    <% category.articles.each do |article| %>
      <% @article = article %>
      <li><%= link_to @article.title, category_article_path(@article.category, @article) %></li> <!-- want to use name vs id -->
    <% end %>
  </ul>
<% end %>


